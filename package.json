{
  "name": "mirage-encryption",
  "version": "0.0.1",
  "description": "A utility package for implementing MongoDB Client-Side Field Level Encryption (CSFLE) with support for multiple KMS providers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
